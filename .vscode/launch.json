{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "al",
            "request": "launch",
            "name": "AL Exercise Container",
            "server": "http://ALGO",
            "serverInstance": "BC",
            "authentication": "Windows",
            "startupObjectId": 22,
            "startupObjectType": "Page",
            "breakOnError": true,
            "launchBrowser": true,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true
        },
        {
            "name": "Attach to AL Exercise Container",
            "type": "al",
            "request": "attach",
            "server": "http://ALGO/",
            "serverInstance": "BC",
            "authentication": "Windows",
            "breakOnError": true,
            "breakOnRecordWrite": true,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "breakOnNext": "WebClient"
        }
    ]
}